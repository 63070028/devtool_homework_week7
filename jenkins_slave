pipeline {
    agent any
    parameters {
        string(name: 'NAME', defaultValue: 'name', description: 'Enter your name')
        string(name: 'Ages', defaultValue: '0', description: 'Enter your ages')
    }
    stages {
        stage('Example') {
            steps {
               python myscript.py Alice --age 30
            }
        }
    }
}
